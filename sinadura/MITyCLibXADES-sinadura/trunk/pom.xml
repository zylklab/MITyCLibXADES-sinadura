	</licenses>
	<organization>
		<name>Ministerio de Industria, Turismo y Comercio</name>
		<url>http://www.mityc.es</url>
	</organization>
	<repositories>
		<repository>
			<id>sinadura-micyt</id>
			<name>sinadura-micyt</name>
			<url>https://services.zylk.net/nexus/content/repositories/sinadura-micyt</url>
